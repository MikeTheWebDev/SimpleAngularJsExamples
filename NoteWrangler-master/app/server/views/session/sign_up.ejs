  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Note Wrangler</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="css/application.css" />
  </head>

  <body>
    <div class="panel hero-cover">
      <div class="panel-content">
        <div class="registration">
          <h2>Register</h2>
          <form class="form" method='POST' action='/registration'>
            <input class="form-input" type="hidden", name="_csrf", value='<%-csrfToken%>'>
            <input class="form-input" type='text' name='name' placeholder='Name'>
            <input class="form-input" type='text' name='username' placeholder='User Name'>
            <input class="form-input" type='password' name='password' placeholder='Password'>
            <input class="form-input" type='password' name='password_confirm' placeholder='Confirm Password'>
            <input class="btn" type='submit' value='Submit'>
            <a href='/sign_in'>Sign in</a>
          </form>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>

